@model personapi_dotnet.Models.Entities.Telefono

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Telefono</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Operador
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Oper)
        </dd>
        <dt class = "col-sm-2">
           Dueño
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Duenio)
        </dd>
        <dt class="col-sm-2">
            Número
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Num)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Num">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
