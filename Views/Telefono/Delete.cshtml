@model personapi_dotnet.Models.Entities.Telefono

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>A¿Está seguro de querer eliminar?</h3>
<div>
    <h4>Telefono</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Operador
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Oper)
        </dd>
        <dt class = "col-sm-2">
            Dueño
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Duenio)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Num" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
